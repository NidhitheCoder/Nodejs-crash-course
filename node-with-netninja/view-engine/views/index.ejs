<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
<body>

  <!-- <% const name="Mario" %> -->
  <%- include('./partials/nav.ejs') %>
  <div class="blogs content">
    <h1>All Blogs</h1>
    <% if(blogs.length > 0) { %>
      <% blogs.forEach(blog => { %>
        <h3 class="title"><%= blog.title %><h3>
        <p class="snippet"><%= blog.snippet %></p>
      <% }) %>
    <% } else { %>
      <p>There are no blogs to display</p>
    <% } %>
  </div>
</body>
</html>